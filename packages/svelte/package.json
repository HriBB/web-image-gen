{
  "name": "web-image-gen-svelte",
  "version": "1.0.3",
  "description": "Svelte component to display image sets generated by web-image-gen.",
  "homepage": "https://github.com/brev/web-image-gen/tree/main/packages/svelte#readme",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Brev Patterson",
    "url": "https://github.com/brev"
  },
  "bugs": {
    "url": "https://github.com/brev/web-image-gen/issues"
  },
  "dependencies": {
    "lazysizes": "^5.3.2",
    "svelte": "^3.55.1",
    "web-image-gen-common": "workspace:^1.0.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0",
    "@sveltejs/kit": "^1.0.11",
    "@sveltejs/package": "^1.0.2",
    "@testing-library/svelte": "^3.2.2",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitest/coverage-c8": "^0.27.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jsdom": "^21.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "prettier-plugin-svelte": "^2.9.0",
    "rimraf": "^3.0.2",
    "svelte-check": "^3.0.2",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.1"
  },
  "keywords": [
    "web",
    "image",
    "generator",
    "svelte",
    "modern",
    "responisve",
    "sizes",
    "formats",
    "tooling",
    "sharp",
    "vite",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brev/web-image-gen.git",
    "directory": "packages/svelte"
  },
  "scripts": {
    "bin:check": "svelte-check --tsconfig ./tsconfig.json",
    "bin:eslint": "eslint --ignore-path .gitignore .",
    "bin:package": "svelte-package",
    "bin:prettier": "prettier --ignore-path .gitignore --plugin-search-dir . .",
    "bin:vitest": "vitest run",
    "build": "run-s sync bin:package",
    "check": "run-s sync bin:check",
    "clean": "rimraf coverage/ package/",
    "dev": "vite dev",
    "format": "run-s 'bin:prettier --write'",
    "lint": "run-s 'bin:prettier --check' bin:eslint",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?\n' && exit 1",
    "sync": "svelte-kit sync",
    "test": "run-s sync 'bin:vitest {1}'",
    "test:cover": "run-s sync 'bin:vitest --coverage'"
  }
}

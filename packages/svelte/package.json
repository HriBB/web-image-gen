{
  "name": "web-image-gen-svelte",
  "version": "1.0.5",
  "description": "Svelte component to display image sets generated by web-image-gen.",
  "homepage": "https://github.com/brev/web-image-gen/tree/main/packages/svelte#readme",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Brev Patterson",
    "url": "https://github.com/brev"
  },
  "bugs": {
    "url": "https://github.com/brev/web-image-gen/issues"
  },
  "dependencies": {
    "lazysizes": "^5.3.2",
    "svelte": "^3.58.0",
    "web-image-gen-common": "workspace:^"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.7",
    "@sveltejs/package": "^2.0.2",
    "@testing-library/svelte": "^3.2.2",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "rimraf": "^5.0.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  },
  "keywords": [
    "web",
    "image",
    "generator",
    "svelte",
    "modern",
    "responisve",
    "sizes",
    "formats",
    "tooling",
    "sharp",
    "vite",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brev/web-image-gen.git",
    "directory": "packages/svelte"
  },
  "scripts": {
    "bin:check": "svelte-check --tsconfig ./tsconfig.json",
    "bin:eslint": "eslint --ignore-path .gitignore .",
    "bin:package": "svelte-package",
    "bin:prettier": "prettier --ignore-path .gitignore --plugin-search-dir . .",
    "bin:vitest": "vitest run",
    "build": "run-s sync bin:package",
    "check": "run-s sync bin:check",
    "clean": "rimraf coverage/ dist/",
    "dev": "vite dev",
    "format": "run-s 'bin:prettier --write'",
    "lint": "run-s 'bin:prettier --check' bin:eslint",
    "sync": "svelte-kit sync",
    "test": "run-s sync 'bin:vitest {1}'",
    "test:cover": "run-s sync 'bin:vitest --coverage'"
  },
  "exports": {
    "./package.json": "./package.json",
    "./ImageSet.svelte": {
      "types": "./dist/ImageSet.svelte.d.ts",
      "svelte": "./dist/ImageSet.svelte",
      "default": "./dist/ImageSet.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "typesVersions": {
    ">4.0": {
      "ImageSet.svelte": [
        "./dist/ImageSet.svelte.d.ts"
      ],
      "index": [
        "./dist/index.d.ts"
      ]
    }
  }
}

{
  "name": "web-image-gen-svelte",
  "version": "1.0.0",
  "description": "Svelte component to display image sets generated by web-image-gen.",
  "homepage": "https://github.com/brev/web-image-gen/tree/main/packages/svelte#readme",
  "license": "MIT",
  "packageManager": "pnpm@7",
  "type": "module",
  "author": {
    "name": "Brev Patterson",
    "url": "https://github.com/brev"
  },
  "bugs": {
    "url": "https://github.com/brev/web-image-gen/issues"
  },
  "dependencies": {
    "lazysizes": "^5.3.2",
    "svelte": "^3.50.1",
    "web-image-gen-common": "workspace:^1.0.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "^1.0.0-next.505",
    "@sveltejs/package": "^1.0.0-next.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/node": "^18.7.23",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitest/coverage-c8": "^0.23.4",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-testing-library": "^5.7.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "rimraf": "^3.0.2",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vitest": "^0.23.4"
  },
  "keywords": [
    "web",
    "image",
    "generator",
    "svelte",
    "modern",
    "responisve",
    "sizes",
    "formats",
    "tooling",
    "sharp",
    "vite",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brev/web-image-gen.git",
    "directory": "packages/svelte"
  },
  "scripts": {
    "bin:check": "svelte-check --tsconfig ./tsconfig.json",
    "bin:eslint": "eslint --ignore-path .gitignore .",
    "bin:package": "svelte-package",
    "bin:prettier": "prettier --ignore-path .gitignore --plugin-search-dir . .",
    "bin:vitest": "vitest run",
    "build": "run-s sync bin:package",
    "check": "run-s sync bin:check",
    "clean": "rimraf coverage/ package/",
    "dev": "vite dev",
    "format": "run-s 'bin:prettier --write'",
    "lint": "run-s 'bin:prettier --check' bin:eslint",
    "prepublishOnly": "echo 'You want to publish `package/`, not `.`!\n' && exit 1",
    "sync": "svelte-kit sync",
    "test": "run-s sync 'bin:vitest {1}'",
    "test:cover": "run-s sync 'bin:vitest --coverage'"
  }
}

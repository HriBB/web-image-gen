{
  "name": "web-image-gen-svelte",
  "version": "2.0.3",
  "description": "Svelte component to display image sets generated by web-image-gen.",
  "homepage": "https://github.com/brev/web-image-gen/tree/main/packages/svelte#readme",
  "license": "MIT",
  "svelte": "./dist/index.js",
  "type": "module",
  "author": {
    "name": "Brev Patterson",
    "url": "https://github.com/brev"
  },
  "bugs": {
    "url": "https://github.com/brev/web-image-gen/issues"
  },
  "dependencies": {
    "lazysizes": "^5.3.2",
    "mime": "^3.0.0",
    "svelte": "^4.2.7"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.2.2",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.1",
    "rimraf": "^5.0.5",
    "svelte-check": "^3.6.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "web",
    "image",
    "generator",
    "svelte",
    "modern",
    "responisve",
    "sizes",
    "formats",
    "tooling",
    "sharp",
    "vite",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brev/web-image-gen.git",
    "directory": "packages/svelte"
  },
  "scripts": {
    "bin:check": "svelte-check",
    "bin:eslint": "eslint --ignore-path .gitignore .",
    "bin:package": "svelte-package",
    "bin:prettier": "prettier --ignore-path .gitignore .",
    "bin:vitest": "vitest run",
    "build": "run-s sync bin:package",
    "check": "run-s sync bin:check",
    "clean": "rimraf coverage/ dist/",
    "dev": "vite dev",
    "format": "run-s 'bin:prettier --write --list-different'",
    "lint": "run-s 'bin:prettier --check' bin:eslint",
    "sync": "svelte-kit sync",
    "test": "run-s sync 'bin:vitest {1}'",
    "test:cover": "run-s sync 'bin:vitest --coverage'"
  },
  "exports": {
    "./package.json": "./package.json",
    "./ImageSet.svelte": {
      "types": "./dist/ImageSet.svelte.d.ts",
      "svelte": "./dist/ImageSet.svelte",
      "default": "./dist/ImageSet.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "ImageSet.svelte": [
        "./dist/ImageSet.svelte.d.ts"
      ],
      "index": [
        "./dist/index.d.ts"
      ]
    }
  }
}
